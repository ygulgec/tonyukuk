# Sözlük üretimi (dict comprehension) testi

# Test 1: Basit sözlük literali
tam s = {"ad": 42, "yas": 25}
yazdır(sözlük_uzunluk(s))

# Test 2: Sözlük üretimi
dizi isimler = ["ali", "veli", "ayse"]
