{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Tonyukuk",
    "scopeName": "source.tonyukuk",
    "patterns": [
        {"include": "#comments"},
        {"include": "#strings"},
        {"include": "#numbers"},
        {"include": "#keywords"},
        {"include": "#types"},
        {"include": "#builtins"},
        {"include": "#constants"},
        {"include": "#operators"},
        {"include": "#decorators"},
        {"include": "#type-annotations"},
        {"include": "#function-definition"},
        {"include": "#class-definition"},
        {"include": "#function-call"},
        {"include": "#identifiers"}
    ],
    "repository": {
        "comments": {
            "patterns": [
                {
                    "name": "comment.line.number-sign.tonyukuk",
                    "match": "#.*$"
                },
                {
                    "name": "comment.block.tonyukuk",
                    "begin": "/\\*",
                    "end": "\\*/",
                    "patterns": [
                        {
                            "name": "comment.block.nested.tonyukuk",
                            "begin": "/\\*",
                            "end": "\\*/"
                        }
                    ]
                }
            ]
        },
        "strings": {
            "patterns": [
                {
                    "name": "string.quoted.double.tonyukuk",
                    "begin": "\"",
                    "end": "\"",
                    "patterns": [
                        {
                            "name": "constant.character.escape.tonyukuk",
                            "match": "\\\\[nrt\\\\\"0]"
                        },
                        {
                            "name": "meta.embedded.expression.tonyukuk",
                            "begin": "\\$\\{",
                            "end": "\\}",
                            "beginCaptures": {
                                "0": {"name": "punctuation.definition.interpolation.begin.tonyukuk"}
                            },
                            "endCaptures": {
                                "0": {"name": "punctuation.definition.interpolation.end.tonyukuk"}
                            },
                            "patterns": [
                                {"include": "#numbers"},
                                {"include": "#keywords"},
                                {"include": "#identifiers"},
                                {"include": "#operators"}
                            ]
                        }
                    ]
                }
            ]
        },
        "numbers": {
            "patterns": [
                {
                    "name": "constant.numeric.float.tonyukuk",
                    "match": "\\b[0-9]+\\.[0-9]+\\b"
                },
                {
                    "name": "constant.numeric.integer.tonyukuk",
                    "match": "\\b[0-9]+\\b"
                }
            ]
        },
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.control.tonyukuk",
                    "match": "\\b(e\u011fer|eger|yoksa|ise|son|d\u00f6ng\u00fc|dongu|iken|d\u00f6nd\u00fcr|dondur|k\u0131r|kir|devam|e\u015fle|esle|durum|varsay\u0131lan|varsayilan|her|i\u00e7in|icin|dene|yakala|f\u0131rlat|firlat|bekle)\\b"
                },
                {
                    "name": "keyword.other.tonyukuk",
                    "match": "\\b(kullan|bu|genel|sabit|uygula|e\u015fzamans\u0131z|eszamansiz|de\u011fi\u015fken|degisken|soyut|tip)\\b"
                },
                {
                    "name": "keyword.operator.logical.tonyukuk",
                    "match": "\\b(ve|veya|de\u011fil|degil)\\b"
                },
                {
                    "name": "keyword.declaration.tonyukuk",
                    "match": "\\b(i\u015flev|islev|s\u0131n\u0131f|sinif|say\u0131m|sayim|aray\u00fcz|arayuz)\\b"
                }
            ]
        },
        "types": {
            "patterns": [
                {
                    "name": "storage.type.tonyukuk",
                    "match": "\\b(tam|ondal\u0131k|ondalik|metin|mant\u0131k|mantik|dizi|s\u00f6zl\u00fck|sozluk)\\b"
                }
            ]
        },
        "builtins": {
            "patterns": [
                {
                    "name": "support.function.builtin.tonyukuk",
                    "match": "\\b(yazd\u0131r|yazdir|uzunluk|ekle|\u00e7\u0131kar|cikar|birle\u015ftir|birlestir|k\u0131rp|kirp|tersle|bul|i\u00e7erir|icerir|mutlak|kuvvet|karek\u00f6k|karekok|min|maks|pi|tam_metin|metin_tam|ondal\u0131k_metin|ondalik_metin|metin_ondal\u0131k|metin_ondalik|sin|cos|tan|log|log10|\u00fcst|ust|taban|tavan|yuvarla|mod|rastgele|satiroku|sat\u0131roku|dosya_oku|dosya_yaz|dosya_ekle|dosya_sat\u0131rlar|dosya_satirlar|b\u00fcy\u00fck_harf|buyuk_harf|k\u00fc\u00e7\u00fck_harf|kucuk_harf|b\u00f6l|bol|tekrarla|ba\u015flar_m\u0131|baslar_mi|biter_mi|de\u011fi\u015ftir|degistir|s\u0131rala|sirala|filtre|e\u015fle|indirge|s\u00f6zl\u00fck_yeni|sozluk_yeni|s\u00f6zl\u00fck_ekle|sozluk_ekle|s\u00f6zl\u00fck_oku|sozluk_oku|s\u00f6zl\u00fck_sil|sozluk_sil|s\u00f6zl\u00fck_var_m\u0131|sozluk_var_mi|\u015fimdi|simdi|saat|dakika|saniye|g\u00fcn|gun|ay|y\u0131l|yil|tarih_metin|json_\u00e7\u00f6z\u00fcmle|json_cozumle|http_al|md5|sha256|base64_kodla|base64_\u00e7\u00f6z|base64_coz|e\u015fle\u015fir_mi|eslesir_mi|e\u015fle\u015fme_bul|eslesme_bul|metin_uzunluk|do\u011frula|dogrula|vt_ac|vt_calistir|vt_sorgula|vt_kapat|is_olustur|is_bekle|kilit_olustur|kilitle|kilit_birak)\\b"
                }
            ]
        },
        "constants": {
            "patterns": [
                {
                    "name": "constant.language.boolean.true.tonyukuk",
                    "match": "\\bdo\u011fru|dogru\\b"
                },
                {
                    "name": "constant.language.boolean.false.tonyukuk",
                    "match": "\\byanl\u0131\u015f|yanlis\\b"
                },
                {
                    "name": "constant.language.null.tonyukuk",
                    "match": "\\bbo\u015f|bos\\b"
                }
            ]
        },
        "operators": {
            "patterns": [
                {
                    "name": "keyword.operator.comparison.tonyukuk",
                    "match": "==|!=|<=|>=|<|>"
                },
                {
                    "name": "keyword.operator.assignment.compound.tonyukuk",
                    "match": "\\+=|-=|\\*=|/=|%="
                },
                {
                    "name": "keyword.operator.assignment.tonyukuk",
                    "match": "="
                },
                {
                    "name": "keyword.operator.arithmetic.tonyukuk",
                    "match": "\\+|-|\\*|/|%"
                },
                {
                    "name": "keyword.operator.pipe.tonyukuk",
                    "match": "\\|>"
                },
                {
                    "name": "keyword.operator.arrow.tonyukuk",
                    "match": "->"
                },
                {
                    "name": "keyword.operator.nullcoalesce.tonyukuk",
                    "match": "\\?\\?"
                }
            ]
        },
        "decorators": {
            "patterns": [
                {
                    "name": "meta.decorator.tonyukuk",
                    "match": "@([a-zA-Z\u00c0-\u024f_][a-zA-Z0-9\u00c0-\u024f_]*)",
                    "captures": {
                        "0": {"name": "entity.name.decorator.tonyukuk"}
                    }
                }
            ]
        },
        "type-annotations": {
            "patterns": [
                {
                    "match": "(->)\\s*(tam|ondal\u0131k|ondalik|metin|mant\u0131k|mantik|dizi|s\u00f6zl\u00fck|sozluk|bo\u015fluk|bosluk)",
                    "captures": {
                        "1": {"name": "keyword.operator.arrow.tonyukuk"},
                        "2": {"name": "storage.type.return.tonyukuk"}
                    }
                },
                {
                    "match": "(:)\\s*(tam|ondal\u0131k|ondalik|metin|mant\u0131k|mantik|dizi|s\u00f6zl\u00fck|sozluk)",
                    "captures": {
                        "1": {"name": "punctuation.separator.type.tonyukuk"},
                        "2": {"name": "storage.type.annotation.tonyukuk"}
                    }
                }
            ]
        },
        "function-definition": {
            "patterns": [
                {
                    "match": "\\b(i\u015flev|islev)\\s+([a-zA-Z\u00c0-\u024f\u0400-\u04ff_][a-zA-Z0-9\u00c0-\u024f\u0400-\u04ff_]*)\\s*\\(",
                    "captures": {
                        "1": {"name": "keyword.declaration.function.tonyukuk"},
                        "2": {"name": "entity.name.function.tonyukuk"}
                    }
                }
            ]
        },
        "class-definition": {
            "patterns": [
                {
                    "match": "\\b(s\u0131n\u0131f|sinif)\\s+([A-Z\u00c0-\u024f\u0400-\u04ff][a-zA-Z0-9\u00c0-\u024f\u0400-\u04ff_]*)",
                    "captures": {
                        "1": {"name": "keyword.declaration.class.tonyukuk"},
                        "2": {"name": "entity.name.class.tonyukuk"}
                    }
                }
            ]
        },
        "function-call": {
            "patterns": [
                {
                    "match": "\\b([a-zA-Z\u00c0-\u024f\u0400-\u04ff_][a-zA-Z0-9\u00c0-\u024f\u0400-\u04ff_]*)\\s*\\(",
                    "captures": {
                        "1": {"name": "entity.name.function.call.tonyukuk"}
                    }
                }
            ]
        },
        "identifiers": {
            "patterns": [
                {
                    "name": "variable.other.tonyukuk",
                    "match": "\\b[a-zA-Z\u00c0-\u024f\u0400-\u04ff_][a-zA-Z0-9\u00c0-\u024f\u0400-\u04ff_]*\\b"
                }
            ]
        }
    }
}
